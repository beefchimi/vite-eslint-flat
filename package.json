{
  "name": "vite-eslint-flat",
  "private": true,
  "version": "0.0.0",
  "description": "Attempt to get eslint-config-love working",
  "author": "Curtis Dulmage",
  "license": "ISC",
  "keywords": [
    "vite",
    "eslint",
    "prettier"
  ],
  "homepage": "https://github.com/beefchimi/vite-eslint-flat#readme",
  "bugs": {
    "url": "https://github.com/beefchimi/vite-eslint-flat/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beefchimi/vite-eslint-flat.git"
  },
  "type": "module",
  "engines": {
    "node": ">=20.13.0",
    "npm": ">=10.5.0"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules/.cache && rm -rf node_modules/.vite",
    "nuke": "npm run clean && rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force",
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "preview": "vite preview"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-love": "^48.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}
